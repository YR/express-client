{
  "name": "express-client",
  "description": "An express.js framework for browser clients",
  "version": "1.0.0",
  "author": "Alexander Pope <alexander.pope@nrk.no>",
  "dependencies": {
    "component-emitter": "1.1.3",
    "debug": "2.0.0",
    "lodash-node": "2.4.1",
    "path-to-regexp": "1.0.1",
    "query-string": "1.0.0",
    "url-utils": "git+ssh://git@github.com:yr/url-utils.git#1.2.0"
  },
  "devDependencies": {
    "buddy": "popeindustries/buddy#2.1.0-beta",
    "expect.js": "0.3.1",
    "mocha": "*",
    "mocha-phantomjs": "*"
  },
  "main": "index.js",
  "repository": "https://github.com/YR/express-client.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "buddy build && mocha-phantomjs test/test-runner.html"
  },
  "buddy": {
    "build": {
      "js": {
        "targets": [{
          "input": "test/src/lib.js",
          "output": "test/lib.js",
          "boilerplate": true,
          "bootstrap": true,
          "alias": {
            "emitter": "./node_modules/component-emitter/index.js"
          }
        }]
      }
    }
  }
}
