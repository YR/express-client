{
  "name": "@yr/express-client",
  "description": "An express.js framework for browser clients",
  "version": "1.10.0",
  "author": "Alexander Pope <alexander.pope@nrk.no>",
  "dependencies": {
    "cookie": "0.1.3",
    "eventemitter3": "1.1.1",
    "debug": "2.1.2",
    "lodash-compat": "3.9.3",
    "path-to-regexp": "1.0.3",
    "query-string": "2.3.0",
    "@yr/url-utils": "1.9.0"
  },
  "devDependencies": {
    "buddy": "*",
    "expect.js": "0.3.1",
    "mocha": "*",
    "mocha-phantomjs": "*"
  },
  "main": "index.js",
  "repository": "https://github.com/YR/express-client.git",
  "license": "MIT",
  "scripts": {
    "test": "buddy build && mocha-phantomjs test/test-runner.html",
    "lint": "NODE_ENV=development node ./node_modules/buddy/bin/buddy -l watch"
  },
  "buddy": {
    "build": {
      "js": {
        "targets": [{
          "input": "test/src/lib.js",
          "output": "test/lib.js",
          "boilerplate": true,
          "bootstrap": true
        }]
      }
    }
  }
}
