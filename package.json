{
  "name": "@yr/express-client",
  "description": "An express.js framework for the browser",
  "version": "2.3.1",
  "author": "Alexander Pope <alexander.pope@nrk.no>",
  "dependencies": {
    "cookie": "0.2.x",
    "eventemitter3": "1.1.x",
    "debug": "2.2.x",
    "object-assign": "4.0.x",
    "path-to-regexp": "1.2.x",
    "query-string": "3.0.x",
    "@yr/url-utils": "2.1.x"
  },
  "devDependencies": {
    "buddy": "*",
    "expect.js": "*",
    "mocha": "*",
    "mocha-phantomjs": "*",
    "transfigure-babel": "*"
  },
  "main": "index.js",
  "repository": "https://github.com/YR/express-client.git",
  "license": "MIT",
  "scripts": {
    "prepublish": "buddy build",
    "test": "buddy build && mocha-phantomjs test/test-runner.html"
  },
  "buddy": {
    "build": {
      "targets": [
        {
          "input": "src/index.js",
          "output": "test/lib.js",
          "boilerplate": true,
          "bootstrap": true
        },
        {
          "input": "src",
          "output": ".",
          "modular": false
        }
      ]
    }
  }
}