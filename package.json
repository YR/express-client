{
  "name": "@yr/express-client",
  "description": "An express.js framework for the browser",
  "version": "2.6.0",
  "author": "Alexander Pope <alexander.pope@nrk.no>",
  "dependencies": {
    "@yr/url-utils": "2.3.x",
    "cookie": "0.3.x",
    "eventemitter3": "2.0.x",
    "debug": "2.2.x",
    "object-assign": "4.1.x",
    "path-to-regexp": "1.5.x",
    "query-string": "4.2.x"
  },
  "devDependencies": {
    "buddy": "5.1.x",
    "buddy-plugin-babel": "6.14.x",
    "expect.js": "*",
    "mocha": "*",
    "mocha-phantomjs": "*"
  },
  "main": "index.js",
  "repository": "https://github.com/YR/express-client.git",
  "license": "MIT",
  "scripts": {
    "prepublish": "buddy build",
    "test": "buddy build && mocha-phantomjs test/test-runner.html"
  },
  "buddy": {
    "build": {
      "targets": [
        {
          "input": "src",
          "output": ".",
          "modular": false
        },
        {
          "input": "src/index.js",
          "output": "test/lib.js"
        }
      ]
    }
  }
}